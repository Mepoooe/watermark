.socials__wrapper
    ul.social__list
        li.social__item.social__item_like
            a(href="#" class="social__link_like").social__link
                |like
            - var soc = [{soc:'fb'}, {soc:'twt'}, {soc:'vk'}]
            ul.social__list-inner
                each item in soc
                    li.social__item
                        a(class="social__link_#{item.soc}").social__link
                            |#{item.soc}